project Overkill is

   for Main use ("overkill-main.adb");

   -- GPS uses this to start the executable
   for Exec_Dir use "bin";

   for Source_Dirs use ("src");
   for Object_Dir use "obj";

   package Builder is
      for Executable ("overkill-main.adb") use "overkill";
   end Builder;

end Overkill;
